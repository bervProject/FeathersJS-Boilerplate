  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 12.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "jest  --forceExit",
    "dev": "ts-node-dev --no-notify src/",
    "lint": "eslint src/**/*.{js,ts} test/**/*.{js,ts} --quiet --fix",
    "start": "yarn run compile && node lib/",
    "start:prod": "node lib/",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
